    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "size-limit": [
    {
      "path": "dist/react-query-auth.cjs.production.min.js",
