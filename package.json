    "@tsconfig/create-react-app": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "dts-cli": "^1.6.3",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
